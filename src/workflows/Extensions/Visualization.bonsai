        <Patch>Patch1Events</Patch>
        <WheelPatch>Patch1WheelDisplacement</WheelPatch>
        <Threshold>0.0003</Threshold>
        <LowPass>1</LowPass>
      </Expression>
      <Expression xsi:type="rx:BehaviorSubject">
        <Name>Patch1TimeSpent</Name>
      </Expression>
